<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="alphabot_invoicing_base_view_company_form" model="ir.ui.view">
        <field name="name">res.company.form.inherit.alphabotfel</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
	 		<xpath expr="//page" position="after">
				<page name="AlphabotFIS" string="Alphabot Fiscal">
					<group>
						<group>
							<field name="alphabot_invoicing_active" />
						</group>
					</group>
				</page>
			</xpath>
        </field>
    </record>
</odoo>
